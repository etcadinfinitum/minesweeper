public class TestGameModel {

    public static void main(String[] args) {
        MinesweeperModel myModel = new MinesweeperModel(30, 30, MinesweeperModel.GameDifficulty.MEDIUM);
        GameSquare[][] myModelState = myModel.getBoardState();
        for (int row = 0; row < myModelState.length; row++) {
            for (int col = 0; col < myModelState[row].length; col++) {
                if (myModelState[row][col].getBomb()) {
                    System.out.print("M ");
                } else if (myModelState[row][col].getHint() > 0) {
                    System.out.print(myModelState[row][col].getHint() + " ");
                } else {
                    System.out.print("- ");
                }
            }
            System.out.println();
        } 
        
    }

}